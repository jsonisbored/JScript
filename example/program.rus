type NewsArticle = {
    content: string,
};

impl String for NewsArticle {
    fn to_string(self: NewsArticle): number {
        self.content
    },
};

console.log("" + NewsArticle({ content: "BREAKING NEWS!"}).to_string()); // -> "BREAKING NEWS!"


