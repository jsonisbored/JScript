type NewsArticle = {
    content: String,
}

impl String for NewsArticle {
    fn string(self: Self): Number {
        self.content
    },
}

console.log(""+NewsArticle({ content: "BREAKING NEWS!"})); // -> "BREAKING NEWS!"


